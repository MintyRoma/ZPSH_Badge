<Window x:Class="ZPSH_Badge.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZPSH_Badge"
        xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
        mc:Ignorable="d"
        Title="Генератор бейджей" Height="450" Width="800"
        MinHeight="450" MinWidth="800">
    <Grid ShowGridLines="True">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50px"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <StackPanel Background="Black">
            <Button Height="50px" Width="50px" Background="Black" BorderThickness="0" Foreground="Blue" Click="Button_Click">
                <Image Source="./Resources/Images/contacts-book-line.png" Width="24px"></Image>
            </Button>
            <Button Height="50px" Width="50px" Background="Black" BorderThickness="0"></Button>
            <Button Height="50px" Width="50px" Background="Black" BorderThickness="0"></Button>
        </StackPanel>
        <Frame Grid.Column="1" x:Name="MainFrame" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" NavigationUIVisibility="Hidden">

        </Frame>
    </Grid>
</Window>
